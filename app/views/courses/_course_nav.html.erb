<div id="dash_grid">

  <%= link_to content_course_path(@course) do %> 
    <div id="dash_tab">
      <div id="dash_heading">
        <h3>Contents</h3>
      </div>  
      <center><i class="fa fa-book fa-2x icons_100" id="tab_icon"></i></center> 
    </div>
  <% end %>

  <%= link_to course_enrolments_path(@course) do %> 
    <div id="dash_tab">
      <div id="dash_heading">
        <h3>Enrol</h3>
      </div>  
      <center><i class="fa fa-check-square-o fa-2x icons_100" id="tab_icon"></i></center> 
    </div>
  <% end %>

  <%= link_to @course.profile do %>
    <div id="dash_tab">
      <div id="dash_heading">
        <h3>Profile</h3>
      </div>  
      <center><i class="fa fa-smile-o fa-2x icons_100" id="tab_icon"></i></center> 
    </div>
  <% end %>

  <%= link_to course_course_items_path(@course) do %>
    <div id="dash_tab">
      <div id="dash_heading">
        <h3>Prices</h3>
      </div>  
      <center><i class="fa fa-money fa-2x icons_100" id="tab_icon"></i></center> 
    </div>
  <% end %>

  <%= link_to settings_course_path(@course) do %>
    <div id="dash_tab">
      <div id="dash_heading">
        <h3>Settings</h3>
      </div>  
      <center><i class="fa fa-cog fa-2x icons_100" id="tab_icon"></i></center> 
    </div>
  <% end %>

</div>
